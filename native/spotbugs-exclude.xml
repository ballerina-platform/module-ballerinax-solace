<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Suppress EI_EXPOSE_REP and EI_EXPOSE_REP2 for SecureSocketConfig -->
    <!-- The List fields use List.of() which returns unmodifiable lists, so these warnings are false positives -->
    <Match>
        <Class name="io.ballerina.lib.solace.config.ssl.SecureSocketConfig"/>
        <Or>
            <Bug pattern="EI_EXPOSE_REP"/>
            <Bug pattern="EI_EXPOSE_REP2"/>
        </Or>
    </Match>
    <Match>
        <Class name="io.ballerina.lib.solace.listener.MessageDispatcher"/>
        <Bug pattern="SF_SWITCH_NO_DEFAULT"/>
    </Match>
    <Match>
        <Class name="io.ballerina.lib.solace.listener.MessageReceiver"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="io.ballerina.lib.solace.listener.OnErrorCallback"/>
        <Bug pattern="DM_EXIT"/>
    </Match>
    <Match>
        <Class name="io.ballerina.lib.solace.listener.Service"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Class name="io.ballerina.lib.solace.consumer.MessageConverter" />
        <Bug pattern="BC_UNCONFIRMED_CAST" />
    </Match>
</FindBugsFilter>
